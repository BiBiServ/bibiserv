             </xs:sequence>
             <xs:sequence minOccurs="2" maxOccurs="unbounded">
                  <xs:element ref="bt:abstractAlignedDnaSequence"/>
             </xs:sequence>
             <xs:sequence minOccurs="2" maxOccurs="unbounded">

   <xs:complexType name="crossRefType">
      <xs:annotation>
         <xs:documentation>An element of type 'crossRefType' can be used to add a reference to a source of an object of biological data, by giving either its lsid or a database name and Entry Id (and optionally a DB URL)</xs:documentation>
      </xs:annotation>
        <xs:sequence>
             <xs:choice>
                  <xs:sequence>
                       <xs:element name="databaseName" type="xs:string">
                            <xs:annotation>
                                 <xs:documentation>name of the database, should be from a controled vocabulary, to be done</xs:documentation>
                            </xs:annotation>
                       </xs:element>
                       <xs:element name="entryID" type="xs:string"/>
                       <xs:element name="databaseURL" type="xs:string" minOccurs="0"/>
                  </xs:sequence>
                  <xs:element name="lsid" type="bt:lsid"/>
             </xs:choice>
        </xs:sequence>
   </xs:complexType>

